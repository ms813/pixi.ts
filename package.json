{
  "name": "pixi-webpack-ts",
  "version": "5.0.1",
  "license": "CC-BY-NC-4.0",
  "author": "Lo√Øc TRUCHOT <loic.truchot.pro@gmail.com>",
  "description": "A pre installed environnement to work with pixi.js with webpack & typescript",
  "engines": {
    "node": "~8.11.0",
    "npm": "~5.8.0"
  },
  "scripts": {
    "npm-reset": "rm -rf node_modules && npm cache verify && npm i",
    "git-reset": "git reset HEAD --hard && git clean -fd && git fetch --all --tags --prune && git pull",
    "start": "webpack-dev-server",
    "build": "webpack",
    "test": "jest"
  },
  "keywords": [
    "pixijs",
    "playground",
    "basic",
    "webpack",
    "typescript",
    "seed"
  ],
  "dependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "moment": "^2.24.0",
    "pixi.js": "4.8.8",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.0.4"
  },
  "devDependencies": {
    "@types/node": "9.6.0",
    "@types/pixi.js": "4.8.8",
    "awesome-typescript-loader": "5.0.0-1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "^3.2.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "5.5.0",
    "style-loader": "^1.0.0",
    "tsconfig-paths-webpack-plugin": "3.0.2",
    "tslint": "5.9.1",
    "typescript": "3.5.3",
    "webpack": "4.39.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "app/$1"
    },
    "globals": {
      "PIXI": "pixi.js",
      "ts-jest": {
        "tsConfig": {
          "importHelpers": true
        }
      }
    },
    "setupFiles": [
      "./jest-setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
